  cursor: pointer;
  margin: 5px;
}
.login-Ficon {
  color: #1a237e;
}
.login-Gicon {
  color: #d50000;
}
.login-Ficon:hover,
.login-Gicon:hover {
  opacity: 0.9;
}
.forgot-password {
  color: #000000;
  text-decoration: underline;
}
.forgot-password:hover {
  text-decoration: none;
}
.register-btn {
  width: 100%;
  height: 30px;
